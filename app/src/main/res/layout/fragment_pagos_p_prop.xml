<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools">


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageView_p_prop"
                android:layout_width="408dp"
                android:layout_height="160dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.025"
                app:srcCompat="@drawable/ic_citris" />

            <LinearLayout
                android:id="@+id/ln_finca_nombre_prop"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="10"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView_p_prop">


                <ImageView
                    android:id="@+id/imageVie4"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="6"
                    app:srcCompat="@drawable/ic_rancho" />

                <TextView
                    android:id="@+id/tv_datos_p_prop_finca_nombre"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:layout_weight="4"
                    android:fontFamily="@font/raleway_semibold"
                    android:paddingStart="15dp"
                    android:paddingLeft="15dp"
                    android:text="nombre "
                    android:textSize="20sp" />


            </LinearLayout>


            <LinearLayout
                android:id="@+id/ln_finca_extension_prop"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="10"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/ln_finca_nombre_prop">


                <ImageView
                    android:id="@+id/imageView11"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="6"
                    app:srcCompat="@drawable/ic_cinta_metrica" />

                <TextView
                    android:id="@+id/tv_datos_p_prop_finca_extension"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="4"
                    android:gravity="center_vertical"
                    android:paddingStart="15dp"
                    android:paddingLeft="15dp"
                    android:text="0"
                    android:textSize="20sp" />


            </LinearLayout>

            <LinearLayout
                android:id="@+id/ln_datos_cap_prod_pro"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="10"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/ln_finca_extension_prop">


                <ImageView
                    android:id="@+id/imageView_prop"
                    android:layout_width="94dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="4"
                    app:srcCompat="@drawable/ic_mapa" />

                <TextView
                    android:id="@+id/tv_datos_p_prop_finca_ubicacion"
                    android:layout_width="181dp"
                    android:layout_height="43dp"
                    android:layout_marginTop="22dp"
                    android:layout_weight="6"
                    android:fontFamily="@font/raleway_semibold"
                    android:gravity="center_vertical"
                    android:paddingStart="15dp"
                    android:paddingLeft="15dp"
                    android:text="Ubicacion"
                    android:textSize="20sp"
                    tools:ignore="RtlSymmetry" />

            </LinearLayout>


            <LinearLayout
                android:id="@+id/ln_p_prop_cant_animales"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="10"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/ln_datos_cap_prod_pro">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="5"
                    android:orientation="vertical"
                    android:weightSum="10"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/ln_datos_cap_prod">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8dp"
                        android:fontFamily="@font/raleway_semibold"
                        android:gravity="left"
                        android:text="Total Animales :"
                        android:textSize="20sp"
                        android:textStyle="bold" />

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        app:srcCompat="@drawable/ic_toro" />
                </LinearLayout>


                <TextView
                    android:id="@+id/tv_det_p_prop_estad_cant_animales"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="8dp"
                    android:layout_weight="5"
                    android:gravity="center_vertical"
                    android:paddingStart="15dp"
                    android:paddingLeft="15dp"
                    android:text="0 "
                    android:textSize="20sp" />

            </LinearLayout>


            <LinearLayout
                android:id="@+id/ln_p_prop_cant_anml"
                android:layout_width="190dp"
                android:layout_height="121dp"
                android:orientation="vertical"
                android:weightSum="10"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/ln_p_prop_cant_animales"
                app:layout_constraintVertical_bias="0.047">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/raleway_semibold"
                    android:gravity="center"
                    android:text="Cantidad De Bovinos"
                    android:textSize="15dp"
                    android:textStyle="bold" />

                <ImageView
                    android:id="@+id/img_bovinos_prop"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left"
                    android:layout_weight="4"
                    app:srcCompat="@drawable/ic_vacal" />


                <TextView
                    android:id="@+id/tv_det_p_prop_cant_vacas"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:paddingStart="15dp"
                    android:paddingLeft="15dp"
                    android:text="0"
                    android:textSize="15dp" />

            </LinearLayout>


            <LinearLayout
                android:id="@+id/ln_p_prop_cant_porcinos"
                android:layout_width="190dp"
                android:layout_height="121dp"
                android:orientation="vertical"
                android:weightSum="10"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintStart_toEndOf="@+id/ln_p_prop_cant_anml"
                app:layout_constraintTop_toBottomOf="@+id/ln_p_prop_cant_animales"
                app:layout_constraintVertical_bias="0.047">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/raleway_semibold"
                    android:gravity="center"
                    android:text="Cantidad De Porcinos"
                    android:textSize="15dp"
                    android:textStyle="bold" />

                <ImageView
                    android:id="@+id/img_bovino_prop"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left"
                    android:layout_weight="4"
                    app:srcCompat="@drawable/ic_silueta_de_vista_lateral_de_cerdo" />


                <TextView
                    android:id="@+id/tv_det_prop_estad_cant_porcinos"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:paddingStart="15dp"
                    android:paddingLeft="15dp"
                    android:text="0"
                    android:textSize="15dp" />

            </LinearLayout>


            <LinearLayout
                android:id="@+id/ln_p_prop_cant_equinos"
                android:layout_width="190dp"
                android:layout_height="121dp"
                android:orientation="vertical"
                android:weightSum="10"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/ln_p_prop_cant_porcinos"
                app:layout_constraintVertical_bias="0.047">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/raleway_semibold"
                    android:gravity="center"
                    android:text="Cantidad De Equinos"
                    android:textSize="15dp"
                    android:textStyle="bold" />

                <ImageView
                    android:id="@+id/img_equinos_prop"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left"
                    android:layout_weight="4"
                    app:srcCompat="@drawable/ic_caballo_de_pie_sobre_tres_patas_en_forma_negra_de_vista_lateral" />


                <TextView
                    android:id="@+id/tv_det_p_prop_estad_cant_equinos"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:paddingStart="15dp"
                    android:paddingLeft="15dp"
                    android:text="0"
                    android:textSize="15dp" />

            </LinearLayout>


            <LinearLayout
                android:id="@+id/ln_p_prop_cant_novillos"
                android:layout_width="190dp"
                android:layout_height="121dp"
                android:orientation="vertical"
                android:weightSum="10"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintStart_toEndOf="@+id/ln_p_prop_cant_anml"
                app:layout_constraintTop_toBottomOf="@+id/ln_p_prop_cant_porcinos"
                app:layout_constraintVertical_bias="0.047">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/raleway_semibold"
                    android:gravity="center"
                    android:text="Cantidad De Novillos (a)"
                    android:textSize="15dp"
                    android:textStyle="bold" />

                <ImageView
                    android:id="@+id/img_novillos_prop"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left"
                    android:layout_weight="4"
                    app:srcCompat="@drawable/ic_signo_de_vista_lateral_de_toro_tauro" />


                <TextView
                    android:id="@+id/tv_p_prop_estad_cant_novillos"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:paddingStart="15dp"
                    android:paddingLeft="15dp"
                    android:text="0"
                    android:textSize="15dp" />

            </LinearLayout>


            <LinearLayout
                android:id="@+id/ln_p_prop_cant_toros"
                android:layout_width="190dp"
                android:layout_height="121dp"
                android:orientation="vertical"
                android:weightSum="10"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/ln_p_prop_cant_novillos"
                app:layout_constraintVertical_bias="0.047">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/raleway_semibold"
                    android:gravity="center"
                    android:text="Cantidad De Toros"
                    android:textSize="15dp"
                    android:textStyle="bold" />

                <ImageView
                    android:id="@+id/img_toros_prop"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left"
                    android:layout_weight="4"
                    app:srcCompat="@drawable/ic_toro_mirando_a_la_derecha" />

                <TextView
                    android:id="@+id/tv_p_prop_estad_cant_toros"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:paddingStart="15dp"
                    android:paddingLeft="15dp"
                    android:text="0"
                    android:textSize="20sp" />

            </LinearLayout>


            <LinearLayout
                android:id="@+id/ln_p_prop_cant_crias"
                android:layout_width="190dp"
                android:layout_height="121dp"
                android:orientation="vertical"
                android:weightSum="10"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/ln_p_prop_cant_equinos"
                app:layout_constraintVertical_bias="0.047">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/raleway_semibold"
                    android:gravity="center"
                    android:text="Cantidad De Crias"
                    android:textSize="15dp"
                    android:textStyle="bold" />

                <ImageView
                    android:id="@+id/img_crias_prop"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left"
                    android:layout_weight="4"
                    app:srcCompat="@drawable/ic_silueta_de_vaca" />


                <TextView
                    android:id="@+id/tv_p_prop_cant_crias"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:paddingStart="15dp"
                    android:paddingLeft="15dp"
                    android:text="0"
                    android:textSize="15dp" />

            </LinearLayout>


        </androidx.constraintlayout.widget.ConstraintLayout>

    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>